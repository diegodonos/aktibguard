    "@storybook/addon-controls": "^7.2.1",
    "@storybook/addon-docs": "^7.2.1",
    "@storybook/addon-viewport": "^7.2.1",
    "@storybook/addon-a11y": "^7.2.1",
    "@storybook/addon-interactions": "^7.2.1",
